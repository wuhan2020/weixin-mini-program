.top_page{
  position:fixed;
  width:100%;
  height:400px;
  background: gray;
  opacity: 0.75;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.cover-input{
  width:500px;
  height: 32px;
  font-size: 12px;
  line-height: 18px;
}
